<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="org.example.model.ClienteModel">
        <table name="Cliente" schema="BancoVigo" catalog=""/>
        <attributes>
            <id name="clDni">
                <column name="clDni"/>
            </id>
            <basic name="clNombre">
                <column name="clNombre"/>
            </basic>
            <basic name="clApellido">
                <column name="clApellido"/>
            </basic>
            <basic name="clTelefono">
                <column name="clTelefono"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
