<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.model.SucursalesModel" table="Sucursales" schema="BancoVigo">
        <id name="suCodSucursal" column="suCodSucursal"/>
        <property name="suCiudad" column="suCiudad"/>
        <property name="suActivo" column="suActivo"/>
    </class>
</hibernate-mapping>