<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="app.model.ClienteModel" table="Cliente" schema="BancoVigo">
        <id name="clDni" column="clDni"/>
        <property name="clNombre" column="clNombre"/>
        <property name="clApellido" column="clApellido"/>
        <property name="clTelefono" column="clTelefono"/>
    </class>
</hibernate-mapping>